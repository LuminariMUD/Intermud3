{
  "name": "i3-gateway-client",
  "version": "1.0.0",
  "description": "JavaScript/Node.js client library for the Intermud3 Gateway API",
  "main": "i3-client.js",
  "types": "i3-client.d.ts",
  "files": [
    "i3-client.js",
    "i3-client.d.ts",
    "README.md",
    "example.js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint i3-client.js example.js",
    "format": "prettier --write *.js *.json *.md",
    "prepublishOnly": "npm run lint && npm run format"
  },
  "keywords": [
    "intermud",
    "intermud3",
    "i3",
    "mud",
    "mmo",
    "gaming",
    "websocket",
    "jsonrpc",
    "gateway",
    "client",
    "library"
  ],
  "author": "Intermud3 Gateway Project",
  "license": "MIT",
  "homepage": "https://github.com/your-org/intermud3-gateway",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/intermud3-gateway.git",
    "directory": "clients/javascript"
  },
  "bugs": {
    "url": "https://github.com/your-org/intermud3-gateway/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "browser": {
    "ws": false
  },
  "exports": {
    ".": {
      "import": "./i3-client.js",
      "require": "./i3-client.js",
      "types": "./i3-client.d.ts"
    }
